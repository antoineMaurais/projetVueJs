{"ast":null,"code":"export default {\n  name: \"spellStats\",\n  props: [\"tableauSpells\"],\n  computed: {\n    statistiqueSort() {\n      let value = this.tableauSpells.filter(spell => spell[1].startsWith(this.searchName));\n      return value.length;\n    },\n\n    statistiqueNbLivreDiff() {\n      let value = this.tableauSpells.filter(spell => spell[1].startsWith(this.searchName));\n      let count = 0;\n      let countArrayLivre = [];\n\n      for (const vFor of value) {\n        if (!countArrayLivre.includes(vFor[0])) {\n          count++;\n          countArrayLivre.push(vFor[0]);\n        }\n      }\n\n      return count;\n    },\n\n    statistiqueNbEcoleDiff() {\n      let value = this.tableauSpells.filter(spell => spell[1].startsWith(this.searchName));\n      let count = 0;\n      let countArraySchool = [];\n\n      for (const vFor of value) {\n        if (!countArraySchool.includes(vFor[2])) {\n          count++;\n          countArraySchool.push(vFor[2]);\n        }\n      }\n\n      return count;\n    },\n\n    statistiqueNbBranchesDiff() {\n      let value = this.tableauSpells.filter(spell => spell[1].startsWith(this.searchName));\n      let count = 0;\n      let countArrayBranche = [];\n\n      for (const vFor of value) {\n        if (vFor[3].length != 0) {\n          for (const branche of vFor[3]) {\n            if (!countArrayBranche.includes(branche)) {\n              count++;\n              countArrayBranche.push(branche);\n            }\n          }\n        }\n      }\n\n      return count;\n    },\n\n    statistiqueNbClassesDiff() {\n      let value = this.tableauSpells.filter(spell => spell[1].startsWith(this.searchName));\n      let count = 0;\n      let countArrayClasse = [];\n\n      for (const vFor of value) {\n        if (vFor[3].length != 0) {\n          for (const classe of vFor[4]) {\n            if (!countArrayClasse.includes(classe[0])) {\n              count++;\n              countArrayClasse.push(classe[0]);\n            }\n          }\n        }\n      }\n\n      return count;\n    }\n\n  },\n  methods: {\n    updateSearch(searchName, searchNameBox) {\n      this.searchName = searchName;\n      this.searchNameBox = searchNameBox;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,eAAe;AACbA,MAAI,EAAE,YADO;AAEbC,OAAK,EAAE,CAAC,eAAD,CAFM;AAGbC,UAAQ,EAAE;AACRC,mBAAe,GAAG;AAChB,UAAIC,KAAI,GAAI,KAAKC,aAAL,CAAmBC,MAAnB,CAA2BC,KAAD,IACpCA,KAAK,CAAC,CAAD,CAAL,CAASC,UAAT,CAAoB,KAAKC,UAAzB,CADU,CAAZ;AAGA,aAAOL,KAAK,CAACM,MAAb;AACD,KANO;;AAORC,0BAAsB,GAAG;AACvB,UAAIP,KAAI,GAAI,KAAKC,aAAL,CAAmBC,MAAnB,CAA2BC,KAAD,IACpCA,KAAK,CAAC,CAAD,CAAL,CAASC,UAAT,CAAoB,KAAKC,UAAzB,CADU,CAAZ;AAGA,UAAIG,KAAI,GAAI,CAAZ;AACA,UAAIC,eAAc,GAAI,EAAtB;;AACA,WAAK,MAAMC,IAAX,IAAmBV,KAAnB,EAA0B;AACxB,YAAI,CAACS,eAAe,CAACE,QAAhB,CAAyBD,IAAI,CAAC,CAAD,CAA7B,CAAL,EAAwC;AACtCF,eAAK;AACLC,yBAAe,CAACG,IAAhB,CAAqBF,IAAI,CAAC,CAAD,CAAzB;AACF;AACF;;AACA,aAAOF,KAAP;AACD,KApBO;;AAqBRK,0BAAsB,GAAG;AACvB,UAAIb,KAAI,GAAI,KAAKC,aAAL,CAAmBC,MAAnB,CAA2BC,KAAD,IACpCA,KAAK,CAAC,CAAD,CAAL,CAASC,UAAT,CAAoB,KAAKC,UAAzB,CADU,CAAZ;AAGA,UAAIG,KAAI,GAAI,CAAZ;AACA,UAAIM,gBAAe,GAAI,EAAvB;;AACA,WAAK,MAAMJ,IAAX,IAAmBV,KAAnB,EAA0B;AACxB,YAAI,CAACc,gBAAgB,CAACH,QAAjB,CAA0BD,IAAI,CAAC,CAAD,CAA9B,CAAL,EAAyC;AACvCF,eAAK;AACLM,0BAAgB,CAACF,IAAjB,CAAsBF,IAAI,CAAC,CAAD,CAA1B;AACF;AACF;;AACA,aAAOF,KAAP;AACD,KAlCO;;AAmCRO,6BAAyB,GAAG;AAC1B,UAAIf,KAAI,GAAI,KAAKC,aAAL,CAAmBC,MAAnB,CAA2BC,KAAD,IACpCA,KAAK,CAAC,CAAD,CAAL,CAASC,UAAT,CAAoB,KAAKC,UAAzB,CADU,CAAZ;AAGA,UAAIG,KAAI,GAAI,CAAZ;AACA,UAAIQ,iBAAgB,GAAI,EAAxB;;AACA,WAAK,MAAMN,IAAX,IAAmBV,KAAnB,EAA0B;AACxB,YAAIU,IAAI,CAAC,CAAD,CAAJ,CAAQJ,MAAR,IAAkB,CAAtB,EAAyB;AACvB,eAAK,MAAMW,OAAX,IAAsBP,IAAI,CAAC,CAAD,CAA1B,EAA+B;AAC7B,gBAAI,CAACM,iBAAiB,CAACL,QAAlB,CAA2BM,OAA3B,CAAL,EAA0C;AACxCT,mBAAK;AACLQ,+BAAiB,CAACJ,IAAlB,CAAuBK,OAAvB;AACF;AACF;AACF;AACF;;AACA,aAAOT,KAAP;AACD,KApDO;;AAqDRU,4BAAwB,GAAG;AACzB,UAAIlB,KAAI,GAAI,KAAKC,aAAL,CAAmBC,MAAnB,CAA2BC,KAAD,IACpCA,KAAK,CAAC,CAAD,CAAL,CAASC,UAAT,CAAoB,KAAKC,UAAzB,CADU,CAAZ;